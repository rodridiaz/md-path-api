{
  "compilerOptions": {
    "target": "ES5",
    "downlevelIteration": true,
    "importHelpers": true,
    "lib": ["dom", "es5", "es2015.collection", "es2015.iterable"],
    "module": "CommonJS",
    "outDir": "public",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "sourceMap": true
  },
  "exclude": ["**/*.spec.ts"]
}
