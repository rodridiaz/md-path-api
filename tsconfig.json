{
  "compilerOptions": {
    "target": "es5",
    "downlevelIteration": true,
    "importHelpers": true,
    "lib": ["dom", "es5", "es2015.collection", "es2015.iterable"],
    "module": "commonjs",
    "outDir": "public",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false
  },
  "exclude": ["**/*.spec.ts"]
}
